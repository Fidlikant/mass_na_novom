<form>
    <question>
        <id>1</id>
        <text>What is your full name?</text>
        <type>text</type>
    </question>
    <question>
        <id>2</id>
        <text>What is your date of birth?</text>
        <type>date</type>
    </question>
    <question>
        <id>3</id>
        <text>What is your gender?</text>
        <type>select</type>
        <options>
            <option>Male</option>
            <option>Female</option>
            <option>Other</option>
        </options>
    </question>
    <question>
        <id>5</id>
        <text>What is your phone number?</text>
        <type>text</type>
    </question>
    <question>
        <id>6</id>
        <text>What is your highest level of education?</text>
        <type>select</type>
        <options>
            <option>High School</option>
            <option>Bachelor's Degree</option>
            <option>Master's Degree</option>
            <option>Doctorate</option>
            <option>Other</option>
        </options>
    </question>
    <question>
        <id>7</id>
        <text>What are your hobbies?</text>
        <type>checkbox</type>
        <options>
            <option>Reading</option>
            <option>Traveling</option>
            <option>Cooking</option>
            <option>Sports</option>
            <option>Music</option>
        </options>
    </question>
    <question>
        <id>8</id>
        <text>How many languages do you speak?</text>
        <type>number</type>
    </question>
    <question>
        <id>9</id>
        <text>Please provide your address</text>
        <type>text</type>
    </question>
    <question>
        <id>3</id>
        <text>Do you have any experience with drugs/alcohol?</text>
        <type>radio</type>
        <options>
            <option>Yes</option>
            <option>No</option>
            <option>I dont know</option>
        </options>
    </question>
    <question>
        <id>11</id>
        <text>How much are you satisfied with this form? 1 - the best, 5 - the worst</text>
        <type>slider</type>
    <question>
        <id>4</id>
        <text>What is your favourite color?</text>
        <type>text</type>
    </question>
    </question>
</form>
